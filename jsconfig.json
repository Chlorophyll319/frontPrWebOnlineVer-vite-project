  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "moduleResolution": "bundler",
    "target": "ES2020",
    "module": "ESNext",
    "jsx": "preserve",
    "types": ["vite/client"]
  },
  "include": ["src/**/*", "types/**/*.d.ts", "types/auto-imports.d.ts"]
}
